    "def square_number(n):\n",
    "    return n ** 2\n",
    "\n",
    "sample_list = [4, 5, 2, 9]\n",
    "\n",
    "squared_list = list(map(square_number, sample_list))\n",
    "\n",
    "print(\"Sample list:\", sample_list)\n",
    "print(\"Square of list elements:\")\n",
    "print(squared_list)\n"
   ]
